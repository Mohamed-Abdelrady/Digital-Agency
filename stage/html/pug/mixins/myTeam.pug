include ../varriables/myTeam
mixin ourteams(teams)
    each team in teams
        .col-lg-3.col-md-4.col-6
            .card.border-0.bg-white.overflow-hidden
                img.card-img-top.w-100.h-100(src=team.img)
                .card-body
                    h5.card-title=team.name
                    p.card-text= team.job